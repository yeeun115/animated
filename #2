#include <stdio.h>
int main(void)
{
	int x;
	printf("십진수 입력바람:");
	scanf("%d", &x);

	int b=1<<7;
	printf("이진수:");

	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");
	b = b >> 1;
	(x & b) == 0 ? printf("0") : printf("1");





	return 0;



}
